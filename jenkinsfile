pipeline {
    agent any
    tools {
        maven 'MAVEN_HOME' // Name of the Maven installation defined in Jenkins
    }
    stages {
        /*stage('Checkout') {
            steps {
                git 'https://github.com/ImenMessaoudi/devops'
            }
        }*/
        
        stage('Build') {
    
            steps {
                sh mvn -version
               // sh 'mvn clean package'
            }
        }
        
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        
        stage('Deploy') {
            steps {
                sh 'mvn deploy'
            }
        }
    }
}
